@model List<core_group_ex_01.Models.User>
<!-- Hue Nhu -->
@{
    ViewData["Title"] = "Users List";
}

<h1>Users List</h1>

<table class="table">
    <thead>
        <tr>
            <th>STT</th>
            <th>Username</th>
            <th>Password</th>
            <th>Role</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model[i].Username</td>
                <td>@Model[i].Password</td>
                <td>@Model[i].Role</td>
            </tr>
        }
    </tbody>
</table>